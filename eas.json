{
  "cli": {
    "version": ">= 12.5.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "OPENAI_KEY": "process.env.OPENAI_KEY"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "OPENAI_KEY": "process.env.OPENAI_KEY"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "OPENAI_KEY": "process.env.OPENAI_KEY"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
